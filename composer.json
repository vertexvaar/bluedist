{
    "name": "vertexvaar/bluedist",
    "type": "project",
    "description": "BlueSprints development and demo distribution",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Oliver Eglseder",
            "email": "php@vxvr.de"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "./packages/*"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0 <8.2",
        "ext-json": "*",
        "vertexvaar/bluesprints": "@dev",
        "vertexvaar/bluefluid": "@dev",
        "vertexvaar/bluemysqlstore": "@dev"
    },
    "autoload": {
        "psr-4": {
            "VerteXVaaR\\BlueDist\\": "src"
        }
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-webdriver": "^1.0.0"
    }
}